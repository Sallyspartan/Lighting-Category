<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<!-- TORCHES AND BRAZIERS PATCHES -->
	<Operation Class="PatchOperationReplace">
		<success>Always</success>
		<xpath>Defs/ThingDef[@Name="TorchTemp"]/designationCategory</xpath>
		<value>
			<designationCategory>Ferny_Lighting</designationCategory>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace" MayRequire="Ludeon.RimWorld.Royalty">
		<success>Always</success>
		<xpath>Defs/ThingDef[@Name="BrazierTemp"]/designationCategory</xpath>
		<value>
			<designationCategory>Ferny_Lighting</designationCategory>
		</value>
	</Operation>
</Patch>